<!DOCTYPE html>
<html>
<head>
	
	{% block headMeta %}
	{% endblock %}
	
	<title>
	{% block pageTitle %}
		Poiz Micro-Shop
	{% endblock %}
	</title>
	
	{% block headStyles %}
		<link type="text/css" rel="stylesheet" media="all"  href="{{ asset('css/font-awesome.min.css') }}"/>
		<link type="text/css" rel="stylesheet" media="all"  href="{{ asset('css/bootstrap.min.css') }}"/>
		<link type="text/css" rel="stylesheet" media="all" href="{{ asset('css/cs_shop.css') }}"/>
		<link type="text/css" rel="stylesheet" media="all" href="{{ asset('css/products.css') }}"/>
	{% endblock %}
	
	{% block headScripts %}
	{% endblock %}
</head>

<body id="pz-body-main" class="pz-body-main">

<div class="header_padder">&nbsp;</div>

<div id="cs_cart_module_wrapper">
	<div id="cs_cart_module" class="col-md-12 no_padding cs_cart_module"></div>
</div>

<div id="static_bar_top" class="navbar navbar-inverse navbar-fixed-top"
     role="navigation">
	<div class="container">
		<div class="navbar-header">
			<div class="pz-header-central">
				<div class="pz-header-central container">
					<div class="pz-wrapper col-md-12 no-padding no-lr-pad">
						
						<div class="pz-logo-block col-md-6 no-l-pad">
							<a href="{{ path('rte_home') }}">
								<img src="{{ asset('images/logos/Poiz_insignia.png') }}" alt="Poiz Logo" class="pz-logo" id="pz-logo" />
							</a>
						</div>
						<div class="pz-avatar-block col-md-6">
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

<div class="admin-menu-block container">
	{% include 'partials/fe-menu.html.twig' %}
</div>

<div class="container">
	<div class="pre-content">
	</div>
</div>

<section id="pz-main-wrapper" class="pz-main-wrapper container">
	<section class="pz-col-12">
		<section>
			<!-- ERRORS -->
		</section>
		<section>
			{% block content %}
			{% endblock %}
		</section>
	</section>
</section>
{% block footerScripts %}
<script src="{{ asset('js/jquery-3.1.0.min.js') }}" type="text/javascript"></script>
<script src="{{ asset('js/jquery.easing.1.3.js') }}" type="text/javascript"></script>
<script src="{{ asset('js/jquery.poiz_tooltip.js') }}" type="text/javascript"></script>
<script src="{{ asset('js/JQAjax.js') }}" type="text/javascript"></script>
<script src="{{ asset('js/pz-test.js') }}" type="text/javascript"></script>
{% endblock %}

</body>
</html>